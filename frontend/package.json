{
  "name": "webpack-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for building front-end projects using Webpack, ES6, SASS and hot reloading with production ready combine, minify and uglify.",
  "main": "index.js",
  "scripts": {
    "watch": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.js --progress --colors",
    "build:watch": "./node_modules/webpack/bin/webpack.js --config webpack.config.js --progress --colors --watch",
    "build": "./node_modules/webpack/bin/webpack.js --config webpack.config.js --progress --colors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Blake Kus <blakekus@gmail.com> (http://kus.io)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kus/webpack-boilerplate.git"
  },
  "keywords": [
    "babel",
    "es2015",
    "webpack",
    "boilerplate"
  ],
  "bugs": {
    "url": "https://github.com/kus/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/kus/webpack-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "clean-webpack-plugin": "^0.1.14",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "string-replace-loader": "^1.0.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
